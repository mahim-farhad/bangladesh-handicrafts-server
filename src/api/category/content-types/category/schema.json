{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "categoryName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "categories"
    },
    "categoryId": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "categorySlug": {
      "type": "uid",
      "targetField": "categoryName",
      "required": true
    }
  }
}
